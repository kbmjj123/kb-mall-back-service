  "paths": {
    "/user/register": {
      "post": {
        "operationId": "CreateUser",
        "responses": {
          "200": {
            "description": "Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseObjectEntity_UserDTO_"
                }
              }
            }
          }
        },
        "description": "创建用户，主要提供为用户自主注册",
        "tags": ["用户模块"],
        "security": [],
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLoginParams"
              }
            }
          }
        }
      }
    },
    "/user/{id}": {
      "get": {
        "operationId": "GetAUser",
        "responses": {
          "200": {
            "description": "Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseObjectEntity_UserWithoutToken_"
                }
              }
            }
          }
        },
        "description": "修改用户信息，主要为用户自主修改",
        "tags": ["用户模块"],
        "security": [],
        "parameters": [
          {
            "description": "用户id",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ]
      }
    },
    "/user/login": {
      "post": {
        "operationId": "CheckUser",
        "responses": {
          "200": {
            "description": "Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseObjectEntity_UserDTO_"
                }
              }
            }
          }
        },
        "description": "用户登录接口",
        "tags": ["用户模块"],
        "security": [],
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLoginParams"
              }
            }
          }
        }
      }
    },
    "/user/refreshToken": {
      "patch": {
        "operationId": "RefreshToken",
        "responses": {
          "200": {
            "description": "Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseObjectEntity__accessToken-string--refreshToken-string__"
                }
              }
            }
          }
        },
        "description": "刷新用户的accessToken以及refreshToken，即延长用户的在线有效性",
        "tags": ["用户模块"],
        "security": [],
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "refreshToken": {
                    "type": "string"
                  }
                },
                "required": ["refreshToken"],
                "type": "object"
              }
            }
          }
        }
      }
    }
